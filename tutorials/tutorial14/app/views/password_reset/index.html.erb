<h1>Forgot your password?</h1>

<%= form_with url: password_reset_path do |form| %>
  
  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email, class: "form-control" %>
  </div>

  <div>
    <%= form.submit :Reset_password, class: "btn btn-sm btn-success mt-3" %>
  </div>
<% end %>

